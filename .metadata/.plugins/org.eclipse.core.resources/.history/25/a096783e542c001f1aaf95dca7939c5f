package kr.co.iei.member.controller;

import java.util.Scanner;

import kr.co.iei.member.model.service.Memberserivce;

public class MemberController {
	private Scanner sc = null;
	private Memberserivce memberSericve;
	public MemberController() {
		super();
		sc = new Scanner(System.in);
		memberSericve = new Memberserivce();
	}
	
	public void main() {
		while(true) {
			System.out.println("\n---------- 회원 관리 프로그램v4 ----------\n");
			System.out.println("1. 전체 회원 조회");
			System.out.println("2. 아이디로 회원 조회");
			System.out.println("3. 이르믕로 회원 조회");
			System.out.println("4. 회원 가입");
			System.out.println("5. 회원 정보 수정");
			System.out.println("6. 회원 탈퇴");
			System.out.print("선택 >> ");
			int select = sc.nextInt();
			switch(select) {
			case 1:
				break;
			case 2:
				break;
			case 3:
				break;
			case 4:
				break;
			case 5:
				break;
			case 6:
				break;
			case 0:
				return;
			default:
				break;
			
			}
		}
	}
	
}
