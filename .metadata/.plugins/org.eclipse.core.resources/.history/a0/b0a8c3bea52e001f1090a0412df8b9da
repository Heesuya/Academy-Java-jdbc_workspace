package kr.co.iei.book.model.vo;

public class Book {
	private int bookNum;
	private String bookGenre;
	private String bookTitle;
	private String booktAuthor;
	private int bookStock;
	public Book() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Book(int bookNum, String bookGenre, String bookTitle, String booktAuthor, int bookStock) {
		super();
		this.bookNum = bookNum;
		this.bookGenre = bookGenre;
		this.bookTitle = bookTitle;
		this.booktAuthor = booktAuthor;
		this.bookStock = bookStock;
	}
	public int getBookNum() {
		return bookNum;
	}
	public void setBookNum(int bookNum) {
		this.bookNum = bookNum;
	}
	public String getBookGenre() {
		return bookGenre;
	}
	public void setBookGenre(String bookGenre) {
		this.bookGenre = bookGenre;
	}
	public String getBookTitle() {
		return bookTitle;
	}
	public void setBookTitle(String bookTitle) {
		this.bookTitle = bookTitle;
	}
	public String getBooktAuthor() {
		return booktAuthor;
	}
	public void setBooktAuthor(String booktAuthor) {
		this.booktAuthor = booktAuthor;
	}
	public int getBookStock() {
		return bookStock;
	}
	public void setBookStock(int bookStock) {
		this.bookStock = bookStock;
	}
	@Override
	public String toString() {
		return "Book [bookNum=" + bookNum + ", bookGenre=" + bookGenre + ", bookTitle=" + bookTitle + ", booktAuthor="
				+ booktAuthor + ", bookStock=" + bookStock + "]";
	}
	
	
	
}
